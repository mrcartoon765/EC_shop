  <meta charset="utf-8">
  <title>会員一覧</title>
</head>
<body>
<table border="1">
<tr>

<th>ID</th>
<th>お名前</th>
<th>メール</th>
<th>性別</th>
<th>登録日時</th>
    <th>編集リンク</th>
</tr>
{% for value in dataArr %}
<tr>
<td>
      <a href="{{constant('create_account\\Bootstrap::ENTRY_URL')}}detail.php?account_id={{value.account_id}}">
      {{value.account_id}}</a>

</td>
<td>
<span style="font-size:60%;">{{value.family_name_kana}} {{value.first_name_kana}}</span><br>
{{value.family_name}} {{value.first_name}}

            </td>
<td>{{value.email}}</td>
        <td>{% if value.sex == '1' %}男性{% elseif value.sex == '2' %}女性{% endif %}</td>

        <td>{{value.regist_date}}</td>


        <td>
<a href="{{constant('create_account\\Bootstrap::ENTRY_URL')}}update.php?account_id={{value.account_id}}">編集</a>
</td>
</tr>
{% endfor %}
</table>
</body>
</html>